{
  "name": "node-trab",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.17.3",
    "file-system": "^2.2.2",
    "yasmij": "^0.2.6"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
